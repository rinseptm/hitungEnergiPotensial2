<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="org.d3if4037.hitungenergipotensial.ui.home.HomeFragment"
        android:label="Hitung Pyramid"
        tools:layout="@layout/fragment_home" />
    <action
        android:id="@+id/action_homeFragment_to_aboutFragment"
        app:destination="@id/aboutFragment" />
    <action
        android:id="@+id/action_homeFragment_to_historiFragment"
        app:destination="@id/historiFragment" />
    <fragment
        android:id="@+id/aboutFragment"
        android:name="org.d3if4037.hitungenergipotensial.ui.AboutFragment"
        android:label="About"
        tools:layout="@layout/fragment_about"/>
    <fragment
        android:id="@+id/historiFragment"
        android:name="org.d3if4037.hitungenergipotensial.ui.histori.HistoriFragment"
        android:label="@string/histori"
        tools:layout="@layout/fragment_histori" />
</navigation>